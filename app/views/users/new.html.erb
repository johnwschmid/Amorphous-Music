<div class="col-xs-3 center"></div>
<div id="signUp" class="col-xs-6">
	<h2>Sign Up</h2>
	<%= form_for @user do |f| %>
		<%= f.label :username %>
		<%= f.text_field :username %>
		<%= f.label :email %>
		<%= f.email_field :email %>
		<%= f.label :password %>
		<%= f.password_field :password %>
		<%= f.label :password_confirmation, "Confirmation" %>
		<%= f.password_field :password_confirmation %>
	<% end %>
</div>
<div class="col-xs-3 center"></div>